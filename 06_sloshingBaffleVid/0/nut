FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    tank_wall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    bafflenya_master
    {
        type            nutkWallFunction;
        coupleGroup     bafflenya;
        value           uniform 0;
    }
    bafflenya_slave
    {
        type            nutkWallFunction;
        coupleGroup     bafflenya;
        value           uniform 0;
    }
}


